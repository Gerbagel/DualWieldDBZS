<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:DBZSDualWieldAva.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:DBZSDualWieldAva.Views"
        mc:Ignorable="d" d:DesignWidth="220" d:DesignHeight="180"
        x:Class="DBZSDualWieldAva.Views.SettingsWindow"
        Icon="/Assets/favicon.ico"
        Title="Settings" Width="220" Height="180">
	<Window.DataContext>
		<vm:SettingsViewModel/>
	</Window.DataContext>
    <Window.Styles>
        <StyleInclude Source="/Assets/Styles.axaml"/>
    </Window.Styles>
	<Window.Background>
		<ImageBrush Source="/Assets/background.png" TileMode="Tile" Stretch="None" DestinationRect="0,0,16,16"/>
	</Window.Background>
    <views:SettingsView />
</Window>
