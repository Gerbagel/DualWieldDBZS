        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:DBZSDualWieldAva.Views"
        mc:Ignorable="d" d:DesignWidth="250" d:DesignHeight="210"
